#!/bin/sh

. auto/options
. auto/init
. auto/sources

test -d $OBJS || mkdir $OBJS
echo > $NGX_AUTO_CONFIG_H

if [ "$PLATFORM" != win32 ]; then
    . auto/headers
fi

. auto/os/conf
. auto/modules

. auto/cc
. auto/lib/conf

if [ "$PLATFORM" != win32 ]; then
    . auto/threads
fi

. auto/make
. auto/lib/make

if [ "$PLATFORM" != win32 ]; then
    . auto/unix
fi

#have NGX_SMP . auto/have

. auto/summary
